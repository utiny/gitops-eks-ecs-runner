{
  "family": "ecs-runner",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::118542481734:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::118542481734:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {

      "image": "nsactl/runner:1",
      "essential": true,
      "name": "nsactl-runner-container",
      
      "environment": [
        {
          "name": "_PATH",
          "value": "utiny/gitops-eks-ecs-runner"
        },
        {
          "name": "GH_REPOSITORY",
          "value": "https://github.com/utiny/gitops-eks-ecs-runner"
        },
        {
          "name": "GH_RUNNER_LABELS",
          "value": "ProjectA"
        },
        {
          "name": "SCOPE",
          "value": "repos"
        }
      ],
      "secrets": [
        {
          "valueFrom": "arn:aws:ssm:us-east-2:118542481734:parameter/GITHUB_ACCESS_TOKEN",
          "name": "GITHUB_ACCESS_TOKEN"
        }
      ]
    }
  ],

  "requiresCompatibilities": [
    "FARGATE"
  ]
}
